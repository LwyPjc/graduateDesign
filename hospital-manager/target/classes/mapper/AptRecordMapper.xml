<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hospital.appointment.dao.AptRecordMapper">

    <select id="findByOpenid" parameterType="java.lang.String" resultType="com.hospital.appointment.entity.AptRecord">
        select * from apt_record
        <where>
            openid=#{openid,jdbcType=VARCHAR}
        </where>
    </select>
</mapper>
