<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hospital.appointment.dao.DoctorInfoMapper">
    <select id="getByDptId" parameterType="java.lang.String" resultType="com.hospital.appointment.entity.DoctorInfo">
        select * from doctor_info
        <where>
            dpt_id=#{dptId,jdbcType=VARCHAR}
        </where>
    </select>
</mapper>
